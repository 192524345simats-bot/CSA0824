def fact(n):
    if n == 0 or n == 1:
        return 1
    if n > 1:
        return n * fact(n - 1)

# main program 
num = int(input("Enter the number: "))
print(f"Factorial = {fact(num)}")


OUT PUT 
Enter the number: 56
Factorial = 710998587804863451854045647463724949736497978881168458687447040000000000000
